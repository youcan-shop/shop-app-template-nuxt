{
  "type": "module",
  "private": true,
  "engineStrict": true,
  "engines": {
    "node": ">=22.0.0 <23.0.0"
  },
  "scripts": {
    "build": "nuxt build",
    "predev": "prisma generate && prisma migrate deploy",
    "dev": "pnpm nuxt prepare && youcan app dev",
    "generate": "youcan app generate",
    "youcan": "youcan",
    "nuxt": "nuxt",
    "eslint": "eslint . --cache",
    "prepare": "pnpm simple-git-hooks",
    "eslint:fix": "eslint . --cache --fix",
    "stylelint": "stylelint ./app/**/*.vue --cache",
    "stylelint:fix": "stylelint ./app/**/*.vue --cache --fix",
    "lint": "pnpm run eslint && pnpm run stylelint",
    "lint:fix": "pnpm run eslint:fix && pnpm run stylelint:fix"
  },
  "dependencies": {
    "@nuxt/eslint": "1.9.0",
    "@prisma/client": "^6.16.2",
    "@prisma/nuxt": "0.3.0",
    "@youcan/celeste": "^0.6.54",
    "@youcan/celeste-icons": "^0.6.54",
    "@youcan/celeste-tokens": "^0.6.54",
    "@youcan/qantra": "latest",
    "isbot": "^5.1.30",
    "jsonwebtoken": "^9.0.2",
    "nuxt": "^4.1.2",
    "prisma": "^6.16.2",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.3.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@unocss/eslint-plugin": "^66.5.1",
    "@unocss/nuxt": "66.5.1",
    "@youcan/app": "latest",
    "@youcan/cli": "latest",
    "eslint": "^9.35.0",
    "eslint-plugin-format": "^1.0.1",
    "lint-staged": "^16.1.6",
    "simple-git-hooks": "^2.13.1",
    "stylelint": "^16.24.0",
    "stylelint-config-property-sort-order-smacss": "^10.0.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "unocss": "^66.5.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,vue,json,yaml,yml,md}": "pnpm run eslint:fix",
    "*.vue": "pnpm run stylelint:fix"
  }
}
